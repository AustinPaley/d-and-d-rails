<%= form_for @character do |f| %>
  <br></br>
  <img class="character_name" src = "/../assets/character_name_top.png">
    <br>
  <%= f.text_field :name, class: "character_name" %>
    <br>
  <img class= "character_name" src = "/../assets/character_name_bottom.png" >
    <br>
  <%= f.label "Character Class" %>
  <%= f.collection_select :char_class_id, @char_classes, :id, :name %>
  <br><br>
  <%= f.label "Character Race" %>
  <%= f.collection_select :char_race_id, @char_races, :id, :name %>
  <br><br>
  <%= f.label "Alignment" %>
  <%= f.collection_select :alignment_id, @alignments, :id, :name %>
  <br><br>
  <%= f.label :level %>
  <%= f.number_field :level%>
  <br>
  <%= f.label :experience %>
  <%= f.number_field :experience %>
  <br><br>
  <%= f.label :strength %>
    <br>
  <%= f.number_field :strength %>
    <br><br><br>
  <%= f.label :dexterity %>
    <br>
  <%= f.number_field :dexterity %>
    <br><br><br>
  <%= f.label "Constitution" %>
    <br>
  <%= f.number_field :consitution %>
    <br><br><br>
  <%= f.label :intelligence %>
    <br>
  <%= f.number_field :intelligence %>
    <br><br><br>
  <%= f.label :wisdom %>
    <br>
  <%= f.number_field :wisdom %>
    <br><br><br>
  <%= f.label :charisma %>
    <br>
  <%= f.number_field :charisma %>
    <br><br><br>
  <%= f.submit %>
<% end %>

<%= button_to "Back to Homepage", root_path, {method: :get} %><br>
